<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.zhangmh.whatmobilemanager.Setting">
    <TextView
       style="@style/mytitlestyle"
        android:text="设置中心"/>
    <ListView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/lv_setting_itemlists">

    </ListView>
    <Button
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:textSize="20dp"
        android:padding="25dp"
        android:layout_marginTop="2dp"
        android:id="@+id/bt_setting_locationshowspace"
        android:text="设置归属地显示位置"
        android:background="@drawable/function_greenbutton_normal"
        android:onClick="showLocationSpace"/>
</LinearLayout>
